{
    "name": "Website",
    "version": "1.0.0",
    "description": "Konstantin Tutsch's personal website.",
    "main": "index.js",
    "scripts": {
        "start": "eleventy --serve",
        "build": "if [[ -d ./build ]]; then rm --interactive=never -r ./build; fi && ELEVENTY_ENVIRONMENT=production eleventy",
        "upload": "rsync -rvz --progress --delete ./build/ root@rpi-homeserver:/mnt/storage/konstantintutsch.com",
        "deploy": "npm run build && npm run upload"
    },
