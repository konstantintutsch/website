---
eleventyExcludeFromCollections: true
permalink: "{{ site.locations.opml }}"
---
<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
    <head>
        <title>{{ site.author }}'s Blogroll</title>
    </head>
    <body>
    {% for blog in blogroll %}
        <outline title="{{ blog.name }}" text="{{ blog.name }}" xmlUrl="{{ blog.feed }}" htmlUrl="{{ blog.website }}" type="rss"></outline>
    {% endfor %}
    </body>
</opml>
